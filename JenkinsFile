pipeline{
    agent any

    stages{

        stage ("sonar quality status"){ 

            agent{
                docker{
                    image "maven"
                }
            }
            steps{

                withSonarQubeEnv(credentialsId: 'sonar-tokan') {

                sh 'mvm clean package sonar:sonar'

            }

            
            }
        }
    }
}